{
    "name": "sparklee/learn-doctrine-with-tdd",
    "type": "project",
    "description": "通过 TDD(测试驱动开发) 学习 Doctrine",
    "keywords": [
        "TDD",
        "Doctrine"
    ],
    "license": "MIT",
    "require": {
        "doctrine/dbal": "2.13.9",
        "doctrine/orm": "2.14.1",
        "doctrine/migrations": "^2.2",
        "symfony/cache": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Migrations\\": "database/migrations/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}